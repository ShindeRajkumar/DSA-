#include <iostream>
using namespace std;
int add(int arr[],int n){
    int sum=0;
    for(int i=0;i<n;i++){
    sum=sum+arr[i];
    }
    cout<<sum;
    return sum;
}
int main(){
    int n;
    cout<<"length of array:";
    cin>>n;
int arr[n];
cout<<"Enter the elemnts of array:";
for(int i=0; i<n; i++){
    cin>>arr[i];
}
add(arr,n);
}
